  <!-- Scripts -->
  <script>
    // Smooth scroll for same-page anchors
    document.querySelectorAll('a[href^="#"]').forEach(a=>{
      a.addEventListener('click',e=>{
        const id = a.getAttribute('href');
        if(id.length>1){
          const target = document.querySelector(id);
          if(target){
            e.preventDefault();
            target.scrollIntoView({behavior:'smooth', block:'start'});
          }
        }
      })
    });

    // Attendees modal
    const showMoreBtn = document.getElementById('showMoreBtn');
    if (showMoreBtn) showMoreBtn.addEventListener('click', openAttendeesModal);

    function openAttendeesModal(){
      const modal = document.createElement('div');
      modal.className = 'fixed inset-0 bg-black/50 flex items-center justify-center z-50 p-4';
      modal.innerHTML = `
        <div class="bg-white rounded-lg p-8 max-w-6xl w-full max-h-[90vh] overflow-y-auto">
          <div class="flex justify-between items-center mb-6">
            <h3 class="text-3xl font-bold">All Attendees</h3>
            <button type="button" aria-label="Close attendees list" class="text-gray-500 hover:text-gray-700 text-2xl" onclick="closeAttendeesModal()">&times;</button>
          </div>
          <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-6">
            ${generateAllAttendeeCards()}
          </div>
        </div>`;
      document.body.appendChild(modal);
    }

    function closeAttendeesModal(){
      const modal = document.querySelector('.fixed.inset-0.bg-black\\/50');
      if(modal) modal.remove();
    }

